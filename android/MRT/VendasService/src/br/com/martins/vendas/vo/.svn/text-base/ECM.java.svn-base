package br.com.martins.vendas.vo;

import java.util.HashMap;
import java.util.Map;

import com.brq.mobile.framework.core.BaseVo;

public class ECM extends BaseVo {

	private static final long serialVersionUID = 8181332642510395028L;

	public Integer				codFilialExpedicao;

	public Integer				codMercadoria;

	public Integer				quantidadeMininaVenda;

	public String				flagMultiplicadorQuantidade;

	public Integer				codGrupoFracionado;

	public Integer				quantidadeMinimaKit;

	@Override
	public Map<String, Object> toMap() {
		Map<String, Object> propriedades = new HashMap<String, Object>();
		propriedades.put("codFilialExpedicao", codFilialExpedicao);
		propriedades.put("codMercadoria", codMercadoria);
		propriedades.put("quantidadeMininaVenda", quantidadeMininaVenda);
		propriedades.put("flagMultiplicadorQuantidade", flagMultiplicadorQuantidade);
		propriedades.put("codGrupoFracionado", codGrupoFracionado);
		propriedades.put("quantidadeMinimaKit", quantidadeMinimaKit);
		return propriedades;
	}
}
