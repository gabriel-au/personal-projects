<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/linearLayoutLinhaTempo"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="horizontal"
    android:background="@drawable/fundo_internas"
    android:gravity="top" >


    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" >

        <ImageButton
	        android:id="@+id/button1"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
	        android:padding="90dp"
	        android:layout_marginTop="268dp"
	        android:layout_marginLeft="82dp"
	        android:adjustViewBounds="true"
	        android:background="@drawable/rounded_button_selected"
	        android:scaleType="fitEnd" />
        
        <ImageButton
	        android:id="@+id/button2"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
	        android:padding="85dp"
	        android:layout_marginTop="400dp"
	        android:layout_marginLeft="246dp"
	        android:adjustViewBounds="true"
	        android:background="@drawable/rounded_button"
	        android:scaleType="fitEnd" />
        
        <ImageButton
	        android:id="@+id/button3"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
	        android:padding="85dp"
	        android:layout_marginTop="525dp"
	        android:layout_marginLeft="87dp"
	        android:adjustViewBounds="true"
	        android:background="@drawable/rounded_button"
	        android:scaleType="fitEnd" />
        
        <ImageButton
	        android:id="@+id/button4"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
	        android:padding="85dp"
	        android:layout_marginTop="646dp"
	        android:layout_marginLeft="246dp"
	        android:adjustViewBounds="true"
	        android:background="@drawable/rounded_button"
	        android:scaleType="fitEnd" />
        
        <ImageButton
	        android:id="@+id/button5"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
	        android:padding="85dp"
	        android:layout_marginTop="752dp"
	        android:layout_marginLeft="52dp"
	        android:adjustViewBounds="true"
	        android:background="@drawable/rounded_button"
	        android:scaleType="fitEnd" />

        <ImageButton
            android:id="@+id/buttonBack"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_marginTop="950dp"
            android:layout_marginLeft="80dp"
            android:background="@drawable/rounded_button_back" />
        
        <RelativeLayout
	        android:id="@+id/relativeLayoutContentLinhaTempo"
	        android:layout_width="1390dp"
	        android:layout_height="880dp"
	        android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="480dp"
            android:layout_marginTop="180dp"
            android:background="@android:color/transparent" >
        
            <WebView
				android:id="@+id/webviewBoxContent"
				android:layout_width="fill_parent"
	            android:layout_height="fill_parent"
	            android:visibility="invisible" />
            
		</RelativeLayout>
        
        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginRight="50dp"
            android:layout_marginTop="80dp"
            android:textSize="40dp"
            android:textColor="@android:color/white"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Linha do Tempo" />

    </RelativeLayout>

</LinearLayout>