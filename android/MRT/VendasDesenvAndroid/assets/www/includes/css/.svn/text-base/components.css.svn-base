}
table tbody tr.selected td label {
	color:#000; 
	font-weight:bold;
}

table.itens tbody td {
	height:48px;
}

table tbody td {
	position:relative;
	margin:0; padding:0px 5px 2px 5px; border:1px solid; border-left:0;
	border-top-color: #D7D7D7;border-right-color: #9F9F9F;border-bottom-color: #9F9F9F;
	height:48px;
	text-align:center;
