* {
	margin:0; padding:0; 
	font-family: Tahoma, Verdana, Arial; color:#000000; font-weight:normal;	
	-webkit-tap-highlight-color:rgba(0,0,0,0);
}

/* ESQUELETO */
#container {
	position:absolute; top:0; left:0; overflow: hidden;
	min-width:400px; width:100%;
	background-image:url(../images/bg_body.png);
}

#menu, #header, #mainContent, #footer, .panel, .result, .panelLine {
	position:relative; clear:both; float:left;
	width:100%;
}

#header, #footer {
	height:54px;
}
#header {
	background: -webkit-gradient(linear, 0 0%, 0 100%, from(#055c92), to(#033d61));
	
}
#header {
	border-bottom:1px solid #EE6622;
}
#header.inicial {
	border-bottom:2px solid #EE6622;
}
#content {
	position:absolute; left:0; top:0; z-index:1;
	width:100%;
	background-color:#D7D7D7;
}
#footer {
	border-top:2px solid #335588;
	background: -webkit-gradient(linear, 0 0%, 0 100%, from(#055c92), to(#033d61));
}
#container .panel {
	position:relative; clear:both;
	padding:5px 0; border-top:1px solid #DDDDDD; border-bottom:1px solid #888888;
	/*background: -webkit-gradient(linear, 0 0%, 0 100%, from(#E2E2E2), to(#B8B8B8));*/
	background: -webkit-gradient(linear, 0 0%, 0 100%, from(#FCFCFC), to(#C7C7C7));
}

div.panel {
	position:relative; clear:both;
	padding:5px 0; border-top:1px solid #DDDDDD; border-bottom:1px solid #888888;
	/*background: -webkit-gradient(linear, 0 0%, 0 100%, from(#E2E2E2), to(#B8B8B8));*/
	background: -webkit-gradient(linear, 0 0%, 0 100%, from(#FCFCFC), to(#C7C7C7));
}

#container .panelLine {
	position:relative; clear:both;
	padding:5px 0; border-top:1px solid #DDDDDD; border-bottom:1px solid #888888;
	background: -webkit-gradient(linear, 0 0%, 0 100%, from(#E2E2E2), to(#FFFFFF));
}
#container .paddedBlue, #container .paddedGrey {
	border:0; padding-bottom:13px;
}
#container .paddedBlue {
	/*background-image: url(../images/bg_headerBottom.png), url(../images/bg_texturePanel.png); background-repeat: no-repeat, repeat; background-position: center bottom;*/
	background:-webkit-gradient(linear, 0 0%, 0 30%, from(#224477), to(#225588));
}
#container .paddedGrey {
	background-image: url(../images/bg_headerBottom.png),  url(../images/bg_paddedGrey.png); background-repeat: no-repeat, repeat; background-position: center bottom;
	
}

.left {
	position:relative; float:left;
	margin-left:5px;
	margin-top:2px;
}
.side {
	position:relative; float:left;
}
.panel .left {
	margin:0px 0 0px 5px;
}
.right {
	position:relative; float:right;
	margin-right:5px;
}
.panel .right {
	margin:0px 5px 0px 0;
}

.center {
	min-width:10px; margin:auto;
}

/* TEMA AZUL */
.blue .panel {
	padding:5px 0; border-top:1px solid #446699; border-bottom:1px solid #224477;
	background: -webkit-gradient(linear, 0 0%, 0 10%, from(#224477), to(#225588));
}

.blue .button {
	border-color:#224477;
	background-image: -webkit-gradient(linear,left top, left bottom,color-stop(0.1, #4477CC), color-stop(0.9, #335588), color-stop(1, #446699)); color:#FFFFFF;
}
.blue .switch {
	border-color:#224477;
}
.blue .switch div {
	background-image: -webkit-gradient(linear,left top, left bottom,color-stop(0.1, #4477CC), color-stop(0.9, #335588), color-stop(1, #446699));
}

/* COMPONENTES */

/* BIBLIOTECA DE ELEMENTOS VISUAIS */
br.clear {
	float:none; clear:both;
	margin:0; padding: 0; height:0; width:0;
	line-height:0; font-size:0;
}

.small {
	font-size:12px;
}
.normal {
	font-size:13px;
}
.large {
	font-size:14px;
}
.grid {
	font-size:13px;
	color:#000000; font-weight:bold;
	margin-top:5px;
}

h1{
	color:#FFFFFF;
	text-align: center; line-height: 44px; font-size:14px; font-weight:bold;
}
h2 {
	clear:both;
	padding:0 10px; border-top:1px solid #EE6622;
	line-height:27px; background:#003355; color:#FFFFFF; font-size:13px; 
}

h2 strong {
	color:#FFFFFF; font-weight:bold;
}
h2 span {
	display:block; float:right;
}

#menu h2 {
	/*line-height:27px; background: #3F6CAC; color:#FFFFFF; font-size:13px;*/
	line-height:27px; background: #355b90; color:#FFFFFF; font-size:13px;
}

p {
	margin:5px 0;
	font-size:14px;
}

p strong {
	color:#336699; font-weight: bold;
}

ul {
	list-style:none;
}

/* BotÃµes e links */
a {
	text-decoration:none;
}
a:hover {
	color:#335588;
}
a.button {
	display:block; cursor:pointer;
	min-width:8px; height:28px; border-radius:5px; padding:5px 7px; border:1px solid #888888;
	background-image: -webkit-gradient(linear,left top, left bottom,color-stop(0.1, #F4F4F4), color-stop(0.9, #C4C4C4), color-stop(1, #D1D1D1)); line-height:28px; font-weight:bold; font-size:13px; text-align:center;
}
a.highlight {
	border-color:#224477;
	background:-webkit-gradient(linear, 0 0%, 0 90%, from(#1D79B1), to(#033d61)); color:#FFFFFF;
}
/*a.highlight, a.button:hover {
	border-color:#224477;
	background: -webkit-gradient(linear, 0 0%, 0 90%, from(#1D79B1), to(#224477)); color:#FFFFFF;
	background:-webkit-gradient(linear, 0 0%, 0 90%, from(#1D79B1), to(#033d61)); color:#FFFFFF;
}*/

/* BOTÃ•ES COM ÃCONES */
a.btIcon, a.btIcon:hover {
	background-image: url(../images/sprite.png), -webkit-gradient(linear,left top, left bottom,color-stop(0.1, #C1C1C1), color-stop(0.2, #F4F4F4), color-stop(1, #C1C1C1));
	background-repeat: none, repeat;
	background-position: left top, left top;
}
a.btIcon:hover { /*navegador*/
		background-image: url(../images/sprite.png), -webkit-gradient(linear, 0 0%, 0 90%, from(#1D79B1), to(#033d61));
		height:31px;
}
a.btIcon.off { /*dispositivo*/
	height: 28px;
	background-image: url(../images/sprite.png), -webkit-gradient(linear,left top, left bottom,color-stop(0.1, #C1C1C1), color-stop(0.2, #F4F4F4), color-stop(1, #C1C1C1));
}
a.menu {background-position: -280px -82px; width:26px;}
a.menu:hover {background-position: -320px -82px;} /*navegador*/
a.menu.off {background-position: -280px -82px;} /*dispositivo*/

a.favorite {
	background-position: url(../images/sprite.png) no-repeat -150px -30px,
}
a.favorite:hover {
	background-position: url(../images/sprite.png) no-repeat -120px -30px,
}

a.sync {background: url(../images/sprite.png) no-repeat -7px -124px; border: 0; width:16px;}
a.sync:hover {background: url(../images/sprite.png) no-repeat -47px -124px;}

a.productPromotion {
	display:block; position:absolute; top:0; left:0;
	width:24px; height:24px;
	background: url(../images/sprite.png) no-repeat -180px -120px;
}


#header a.left, #header a.right {
	position:absolute; float:none; top:6px;
}

#header a.left {
	left:0;
}
#header a.right {
	right:0;
}
#footer a.left, #footer a.right {
	margin-top:6px;
}

	/* BotÃµes agrupados horizontalmente */
	.groupButtons {
		position:relative; float:left;
		height:28px; margin:0; border:1px solid #888888; border-radius:5px;
	}
	.groupButtons ul {
		position:relative; display:block; 
		margin:0 auto;
	}
	.groupButtons li, .groupButtons li a {
		display:block; float:left;
		height:28px; line-height:28px;
	}
	.groupButtons li {
		border-left:1px solid #DFDFDF; border-right:1px solid #C3C3C3;
		background-image: -webkit-gradient(linear,left top, left bottom,color-stop(0.1, #F4F4F4), color-stop(0.9, #C4C4C4), color-stop(1, #D1D1D1));
	}
	.groupButtons li a {
		margin:0; padding:0 7px; 
		font-size:13px;
	}
	.groupButtons li.first {
		border-radius:5px 0 0 5px; border-left:0;
	}
	.groupButtons li.last {
		border-radius:0 5px 5px 0; border-right:0;
	}
	.groupButtons li.selected {
		background: -webkit-gradient(linear, 0 0%, 0 90%, from(#336699), to(#224477)); color:#FFFFFF;
	}
	.groupButtons li.selected a {
		color:#FFFFFF;
	}
	.panel .groupButtons {
		float:right; margin-right:5px;
	}


/* FormulÃ¡rios */
label {
	font-size:12px;
}
label.left {
	display:block; float: left;
	line-height: 30px;
}
label.title, span.title {
	color:#000000; font-weight:bold;
}
a.highlight label {
	color:#FFFFFF;
}
span {
	font-size:13px;
	color:#000000; font-weight:bold;
}
span.data {
	font-size:12px;
	color:#000000; font-weight:bold;
}

h2 span {color:#FFFFFF;}
.panel label {
	display:inline-block;
}

#container .paddedBlue label, #container .paddedBlue span, #container .paddedGrey label, #container .paddedGrey span, #footer.blue label, #footer.blue span {
	display:block;
	color:#FFFFFF; text-align:left;
}

#footer label, #footer span {
	display:block; margin:4px 5px;
}
#footer span {
	color: #335588; font-weight:bold;
}

/*input {
	height:28px; border:0; margin:1px 0;
	background:none; font-size:13px; line-height:28px;
}*/
input {
	height:18px; border:0; margin:0 0;
	background:none; font-size:13px; line-height:0;
}

input.search {
	padding-left:30px;
	background: url(../images/bg_searchField.png) no-repeat left center;
	height:19px;
}

.typeInput {
	position:relative;
	width:30%; height:30px; padding:8px 0 0 7px; margin:0px 0px 0 0;
	background-color:#FFFFFF; border-radius:7px; border:1px solid #C4C4C4; -webkit-box-shadow: 0 0 1px 0 #ccc, 0 0 6px 1px #d4d4d4 inset;
}
.typeInput input {
	display:block; float:left;padding-top:0px;
}
.typeInput a {
	display:block; float:right;
	width:30px; height:18px;
	background: url(../images/sprite.png) no-repeat -132px -132px; margin-top: 7px;
}

.inputValue {
	height:14px; border-radius: 5px; border:1px solid #888888; background-color:#FFFFFF; -webkit-box-shadow: inset 1px 1px 2px #B8B8B8; padding:0 5px;
} 

/* CAMPOS DE FORMULARIO */
.formDiv, .formDiv .line {
	position:relative; clear:both; float:left;
	width:100%;
}
.formDiv .line {
	border-top:1px solid #AFAFAF; border-bottom:1px solid #EEEEEE;
	background-color:#EBEBEB;
}
/*.formDiv .line {
	border-top:1px solid #9A9A9A; border-bottom:1px solid #EEEEEE;
	background-color:#EBEBEB;
}*/
.formDiv .off {
	background-color:#C0C0C0;
}
.formDiv .on {
	background-color:#FCFCFC;
}
.formDiv .line .cell {
	float:left; padding:5px 15px 5px 5px;
}

.formDiv .line .cell span, .formDiv .line .cell label {
	display:block;
	line-height:14px;
}
/*SELECT */
div#select, div.options {
	position:absolute; top:0; left:0; z-index:50; width:152px;
	border-radius:5px; border:1px solid #888888; -webkit-box-shadow: 0 0 4px #333333;
	background: -webkit-gradient(linear, 0 0%, 0 100%, from(#E2E2E2), to(#B8B8B8));
}
div#select ul, div#select ul li, div#select ul li a, div.options ul, div.options ul li, div.options ul li a  {
	display:block; font-size:13px; float:left;
}
div#select ul li, div.options ul li {
	padding:5px 0;
	border-top:1px dotted #DDDDDD; border-bottom:1px dotted #888888;
	width:100%;
}
div.options {
	left:auto; right:0;
}
div.options ul li {
	padding:10px 0 !important;
}
div.options ul li label {
	display:block; margin:0 10px !important; width:100px;
}
div#select ul li a {
	margin-left:5px; margin-top:5px;
}
div#select ul li.selected {
	background:#224477;
}
div#select ul li.selected a {
	color:#FFFFFF;
}
div#select ul li.selected {
	background:#224477;
}

