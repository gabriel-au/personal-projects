	font-weight:bold;
}

.tableSelectable td {
	height:40px;
}

.tableList {
	/*position:relative; clear:both; float:left; overflow:hidden;*/
	position:relative; clear:both; float:left;
	width:100%;
} 
.tableList table {
	position:relative;
}
.tableList .nav {
	float:left; clear:both;
	width:100%; height:27px; padding:3px 0;
	background: -webkit-gradient(linear, 0 0%, 0 30%, from(#03263e), to(#03426b));
}
div.nav a.button {
	position:absolute; padding: 0px 5px; z-index: 3; height:25px; width:20px; margin-top:3px; top:0;
	background: #D7D7D7 url(../images/button_seta.png) no-repeat left top;
}
div.nav a.prev {
	left:0; display:none;
	background-position: right top;
}
div.nav a.next {
	right:0;
}


.tableList ul.itemDesc {
	display:block; position: absolute; top:23px; left:0; z-index: 5;
}
.tableList ul.itemDesc li {
	display:block; clear:both; float:left;
	margin:0; padding:0 0 2px 0; height:48px;
	background-color:#D7D7D7;
	border-top:1px solid #D7D7D7; border-bottom: 1px solid #9F9F9F;
}
.tableList ul.itemDesc li.off {
	background-color:#EBEBEB;
}
.tableList ul.itemDesc li.selected {
	background: -webkit-gradient(linear, 0 0%, 0 100%, from(#0878b2), to(#054e74));
	padding:1px 0; border-color:#044161;
	width:3000px;
}
.tableList ul.itemDesc li.selected div.description {
	display:block;
	width:3000px;
}
.tableList ul.itemDesc li a.moreInfo {
	display:block; float:left; 
	height:16px; width:16px; padding:16px 5px; border-right:1px solid #9F9F9F;
}
.tableList ul.itemDesc li.selected a.moreInfo {
	border-color:#044161;
}
.tableList ul.itemDesc li a.moreInfo span.icon {
	margin:0;
}

div.description {
	display:none; float:left; overflow:hidden;
	width:100%; height: 46px;
}
div.description label {
	margin-right:15px;
}

.tableList ul.itemDesc li.selected span, .tableList ul.itemDesc li.selected label {
	color:#FFFFFF; line-height:21px;
}

.tableList ul .right {
	margin-top:5px;
}

.tableList table .right {
	margin-top:8px;
	bottom:2px;
}

