	<script type="text/javascript" src="../includes/js/pages/conversor.js"></script>
	
</head>
<body onload="setTimeout('jsStartConverter()',1000);">
	<!-- CONTAINER, envolve todo conteúdo, caso exista o menu do sistema ele deverá conter a classe 'wMenu' -->
	<div id="container">
		
		<!-- CONTEÚDO, conteúdo das seções da aplicação -->
		<div id="content">
			<!-- CABEÇALHO, topo do aplicativo com a navegação e títulos -->
			<div id="header">
				<!-- Navegação esquerda - em caso de seção com menu, deixar o leftNav vazio -->
				<h1>Conversor</h1>
			</div>
			<!-- FIM CABEÇALHO -->
			
			<!-- CONTEÚDO PRINCIPAL, conteúdo da seção -->
			<div id="mainContent">
				<div class="panel">
					<div class="listPanel">
						<ul id="scroll">			
						</ul>
					</div>
				</div>
				<div class="panel">
					<p id="p-progress" style="text-align: center;">
						<strong>Nome da Tabela:</strong>
						<strong id="strong-arq" style="color: red"></strong>
						<strong>Percentual:</strong>
						<strong id="strong-per" style="color: red"></strong>
					</p>
				</div>
				<a id="bt_continuar" class="button right" style="margin-top: 5px; display: none" href="index.html">Continuar</a>
			</div>
			<!-- FIM CONTEÚDO PRINCIPAL -->
		</div>
		<!-- FIM CONTEÚDO -->
	</div>
</body>
</html>