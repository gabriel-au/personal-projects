<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="restricao_tipo" namespace="/restricao_tipo" extends="default">
		<action name="editar" method="editar"
			class="br.com.xtrategia.fiscon.web.RestricaoTipoAction">
			<result>/WEB-INF/src/restricao_tipo/editar.jsp</result>
		</action>
		<action name="salvar" method="salvar"
			class="br.com.xtrategia.fiscon.web.RestricaoTipoAction">
			<result>/WEB-INF/src/restricao_tipo/listar.jsp</result>
		</action>
		<action name="excluir" method="excluir"
			class="br.com.xtrategia.fiscon.web.RestricaoTipoAction">
			<result>/WEB-INF/src/restricao_tipo/listar.jsp</result>
		</action>
		<action name="listar" method="listar"
			class="br.com.xtrategia.fiscon.web.RestricaoTipoAction">
			<result>/WEB-INF/src/restricao_tipo/listar.jsp</result>
		</action>
	</package>


</struts>
