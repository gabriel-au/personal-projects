<project name="ClassPathAndroid" default="android" basedir="../">

	<!--
	Projeto VendasModulo
	-->
	<target name="android">
		<property name="vendasModulo" value="${basedir}/VendasModulo" />
		<replace file="${vendasModulo}/.classpath" token="&lt;classpathentry kind=&quot;lib&quot; path=&quot;/BRQMobileAndroid/lib/phonegap-1.3.0.jar&quot;/&gt;" value="" />
		<replace file="${vendasModulo}/.classpath" token="&lt;classpathentry kind=&quot;lib&quot; path=&quot;/BRQMobileAndroid/lib/android.jar&quot;/&gt;" value="" />
		<replace file="${vendasModulo}/.classpath" token="BRQMobileWin" value="BRQMobileAndroid" />
		<replace file="${vendasModulo}/.classpath" token="&lt;/classpath&gt;" value="" />
		<echo file="${vendasModulo}/.classpath" append="true">&lt;classpathentry kind=&quot;lib&quot; path=&quot;/BRQMobileAndroid/lib/phonegap-1.3.0.jar&quot;/&gt;</echo>
		<echo file="${vendasModulo}/.classpath" append="true">${line.separator}</echo>
		<echo file="${vendasModulo}/.classpath" append="true">&lt;classpathentry kind=&quot;lib&quot; path=&quot;/BRQMobileAndroid/lib/android.jar&quot;/&gt;</echo>
		<echo file="${vendasModulo}/.classpath" append="true">${line.separator}</echo>
		<echo file="${vendasModulo}/.classpath" append="true">&lt;/classpath&gt;</echo>
		<replaceregexp file="${vendasModulo}/.classpath" match="(\r?\n)\s*\r?\n" flags="g" replace="\1" />

