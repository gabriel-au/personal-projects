package br.com.martins.vendas.vo;

import java.util.HashMap;
import java.util.Map;

import com.brq.mobile.framework.core.BaseVo;

public class CidadesTerritorios extends BaseVo {

	private static final long serialVersionUID = -2480299141752313345L;
	
	public Integer codigoCidade;
	
	public Integer codigoTerritorio;
	
	public String nomeCidade;
	
	public String unidadeFederal;

	@Override
	public Map<String, Object> toMap() {	
		Map<String, Object> propriedades = new HashMap<String, Object>();
		propriedades.put("codigoTerritorio", codigoTerritorio);
		propriedades.put("codigoCidade", codigoCidade);
		propriedades.put("nomeCidade", nomeCidade);
		propriedades.put("unidadeFederal",unidadeFederal);
		
		return propriedades;
	}
	
	
}
