<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="cnh" namespace="/cnh" extends="default">
		<action name="editar" method="editar"
			class="br.com.xtrategia.fiscon.web.CnhAction">
			<result>/WEB-INF/src/cnh/editar.jsp</result>
		</action>
		<action name="salvar" method="salvar"
			class="br.com.xtrategia.fiscon.web.CnhAction">
			<result>/WEB-INF/src/cnh/listar.jsp</result>
		</action>
		<action name="excluir" method="excluir"
			class="br.com.xtrategia.fiscon.web.CnhAction">
			<result>/WEB-INF/src/cnh/listar.jsp</result>
		</action>
		<action name="listar" method="listar"
			class="br.com.xtrategia.fiscon.web.CnhAction">
			<result>/WEB-INF/src/cnh/listar.jsp</result>
		</action>
		<action name="consultar" method="consultar"
			class="br.com.xtrategia.fiscon.web.CnhAction">
			<result>/WEB-INF/src/cnh/consultar.jsp</result>
		</action>
		<action name="exibir" method="exibir"
			class="br.com.xtrategia.fiscon.web.CnhAction">
			<result>/WEB-INF/src/cnh/resultado.jsp</result>
		</action>
	</package>


</struts>
