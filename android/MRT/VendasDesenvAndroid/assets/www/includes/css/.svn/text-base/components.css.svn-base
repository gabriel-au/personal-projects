table {
	font-size:13px;
}
table thead tr {
	background-color:#036395;
}
table thead th{
	height:14px; padding:3px 5px; border-bottom:1px solid #D7D7D7;
	color:#FFFFFF;
}
table tbody tr.off {
	background-color:#EBEBEB;
	height:48px;
}
table tbody tr.selected {
	background-color:#03263E;
}
table tbody tr.highlight {
	background-color:#669900;
}
table tbody tr.highlight, td {
	color:#FFFFFF;
}
table tbody tr.selected td, table tbody tr.selected td span{
