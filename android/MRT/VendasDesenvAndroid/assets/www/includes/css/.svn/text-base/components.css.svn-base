	position:relative; float:left; overflow:hidden; width:100%; height:35px;
}
div.listAtributes {
	position: absolute; left:0; 
}
div.listAtributes div {
	position:relative; float:left;
	margin-right:30px; padding-top:5px;
}
div.listAtributesParent a.button {
	position:absolute; padding: 0 5px; z-index: 3; height:16px; margin-top:1px; top:0;
	background: url(../images/button_seta.png) no-repeat left top;
}
div.listAtributesParent a.prev {
	left:0; display:none;
	background-position: right top;
}
