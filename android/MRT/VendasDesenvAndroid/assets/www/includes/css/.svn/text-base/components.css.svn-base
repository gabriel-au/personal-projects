/* ELEMENTOS DE PÃGINAS */
/* GrÃ¡ficos */
.chart {float:left; width:100%; padding:5px 0;}
.barChart {height:15px; float:right; width:90%; border:3px solid #FFFFFF; background-color:#666666; -webkit-box-shadow: inset 0 0 5px #333333, 0 0 2px #666666; overflow:hidden; border-radius:3px;}
.barChart div {height:15px; width:30%; float:left; background: -webkit-gradient(linear, 0 0%, 0 50%, from(#77AADD), to(#4488BB)); -webkit-box-shadow: 3px 0 5px #000000;}
.barChart div.green {background: -webkit-gradient(linear, 0 0%, 0 50%, from(#00CC00), to(#00AA00));}

/* Filtro por alfabeto */
	.filterAlphabet, .filterAlphabet ul {
		clear:both;
		width:100%; height:50px;
	}
	.filterAlphabet {
		position:absolute; bottom:0;
	    border-top: 1px solid #888888; border-bottom: 1px solid #888888;
		background: -webkit-gradient(linear, 0 0%, 0 100%, from(#E2E2E2), to(#B8B8B8));
	}
	.filterAlphabet li {
		display:block; float:left; cursor:pointer;
		width:7.6%; height:20px; margin:2px 0;
		text-align:center; font-size:13px; line-height:20px;
	}
	.filterAlphabet li.selected{
		background:url(../images/bg_alphaFilterHighlight.png) no-repeat center top; color:#FFFFFF;
	}
	
	/* Switch */
	.switch {
		position:relative; float:right;
		width:60px; height:25px; margin:0px 5px; overflow:hidden;
		background: -webkit-gradient(linear, 0 90%, 0 95%, from(#666666), to(#999999)); border-radius:5px; border: 1px solid #666666;
	}
	.wLabel {
		margin-left:40px;
	}
	label.labelSwitch {
		display: block; position:absolute; top:-2px; left:5px; z-index: 2;
	}
	.switch div {
		position:absolute; left:-30px;
		width:60px; height:25px;
		background-color:#335588; border-radius:5px;
	}
	.on div {
		left:0px;
		margin-left:0;
	}
	.switch div a {
		display:block; float:right;
		width:28px; height:23px; border-radius:3px; border:1px solid #999999;
		background: -webkit-gradient(linear, 0 0%, 0 90%, from(#E4E4E4), to(#787878));
	}
	
	/*ABAS*/
	
	ul.tabs {
		display:block; float:left; clear:both; width:100%;
		background: -webkit-gradient(linear, 0 0%, 0 90%, from(#1D79B1), to(#134F73));
	}
	ul.tabs li {
		display:block; float:left; 
		height:44px; padding:5px 10px; border-left:1px solid #57829c; border-right:1px solid #0e3d5f;
		text-align:center;
	}
	ul.tabs li.selected {
		background: #03263E;
	}
	ul.tabs li span {
		display:block; float:none; margin:3px auto 0;
	}
	ul.tabs li label {
		display:inline-block;
		color:#FFFFFF;
	}
	ul.tabs li:first-child {
		border-left:0;
	}
	ul.tabs li:last-child {
		border-right:0;
	}
	ul.tabs li.selected span.number {
		background-color:#FFFFFF; color:#03263E;
	}
	span.number {
		display:inline-block;
		width:15px; height:15px; border-radius:7px;
		background-color:#0878B2; color:#FFFFFF;
	}
	div.tabMenu {
		float:right; padding:6px 3px 8px 4px;
		border-left: 1px solid #0e3d5f;
		background: -webkit-gradient(linear, 0 0%, 0 90%, from(#1D79B1), to(#134F73));
		
	}
	div.tabMenu a.right {
		margin:0;
	}
	
	
