<%@ page contentType="text/html; charset=ISO-8859-1" %>
<%@ taglib prefix="s" uri="/struts-tags" %>
<%@ taglib prefix="sx" uri="/struts-dojo-tags" %>

<div class="underbanner2"><span>CNH</span></div>
<div id="tabelaleft">
<table>
	<tr class="alt"><td><b>Nº de Registro da CNH</b></td><td><s:property value="pojo.cnh"/> </td></tr>
	<tr><td><b>Data de Nascimento</b></td><td><s:text name="formatar.data">
			<s:param name="value" value="pojo.dataNascimento"/>
		</s:text></td></tr>
	<tr class="alt"><td><b>Nome</b></td><td><s:property value="pojo.nome"/> </td></tr>
	<tr><td><b>Categoria</b></td><td><s:property value="pojo.categoria"/> </td></tr>
	<tr class="alt"><td><b>Data de Validade da CNH</b></td><td><s:text name="formatar.data">
			<s:param name="value" value="pojo.validade"/>
		</s:text></td></tr>
	<tr><td><b>Tipo</b></td><td><s:property value="pojo.tipo"/> </td></tr>
	<tr class="alt"><td><b>Pontuação</b></td><td><s:property value="pojo.pontuacao"/> </td></tr>
</table>
</div>